.x-accordion-item .x-accordion-hd .x-panel-header-text-container {
    background: none repeat scroll 0% 0% #157FCC;
    color: #FFF;
    font-weight: bold;
    font-family: helvetica,arial,verdana,sans-serif;
    text-transform: none;
}


div.olControlAttribution {
  background-color: rgb(220,220,220);
  background-color: rgba(55,55,55,0.2); 
  border-top-left-radius: 5px;
  bottom: 0;
  color: #333;
  max-width: 50%;
  max-height: 50%;
  overflow-y: auto;
  overflow-x: hidden;
  padding: 3px 3px 3px 5px;
  right: 0;
  text-align: right;
  text-shadow: 0 0 2px #fff;
}
div.olControlAttribution a,
div.olControlAttribution a:link,
div.olControlAttribution a:visited,
div.olControlAttribution a:hover,
div.olControlAttribution a:active {
  color: darkblue;
}

div.olControlMousePosition {
    bottom: 20px;
    display: block;
    position: absolute;
    font-family: Arial;
    font-size: smaller;
    background-color: rgb(220,220,220);
    background-color: rgba(55,55,55,0.2);
    border-top-left-radius: 5px;
    bottom: 20px;
    color: #333;
    max-width: 50%;
    max-height: 50%;
    overflow-y: auto;
    overflow-x: hidden;
    padding: 3px 3px 3px 5px;
    right: 0;
    text-align: right;
    text-shadow: 0 0 2px #fff;
}

.x-field-label-cell {
	padding-left: 5px;
	width: 75px;
}

#toolbar-bottom {
	padding: 3px 0px 0px 1px;
