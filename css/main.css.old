/* --- STYLES POUR IMPRESSION --- */

@media print {

	/* --- STYLES DE BASE --- */
	
	/* Page */
	* {
	    margin: 0;
	    padding: 0;
	    border: 0;
	    font-family: sans-serif;
	    font-size: 1em;
	    font-weight: normal;
	    font-style: normal;
	    text-decoration: none;
	}
	html {
	    font-size: 100%; /* Évite un bug d'IE 6-7. (1) */
	}

	body {
	      margin: 0;
	      line-height: 1.4; /* À adapter au design. (4) */
	      color: black;
	      clear:left;
	      background-color: pink;
/*
	      background-color :#fff;
*/
	      
	}
	/* Les éléments qui ne seront pas affichés  */

	#footer {display: none;}
	#accordion {display: none;}
	
	#ficheInfoWms {display: none;}
/*
	#shortcutCombo {display: none;}
	#treePanel {display: none;}
	div.olControlPermalink {display: none;}
	div.olControlMousePosition{display: none;}
	div.olControlPanZoomBar{display: none;}
	.toolbar-top {display: none;}
	#toolbar-bottom {display: none;}
	#treePanel{display: none;}
*/	
	
	/* Header */

	#header {
		margin :0;
		padding: 0;
		float: none !important;
	}
	#mainHeader {
	    font: normal 1em arial,tahoma,sans-serif;
	}
	#mainHeader h2 {
	    font-size: 1.6em;
	    padding-left: 20px;
	}
	#mainHeader h3 {
		font-size: 1.1em;
	    font-style: italic;
	    padding-left: 25px;
	}
	
	
	#map {
		margin :0;
		padding: 0;
		border :none;
		clear:none;
		float: none !important;
	}
	#accordion {
		margin :0;
		padding: 0;
		border :none;
		clear:none;
		float: none !important;
	}
