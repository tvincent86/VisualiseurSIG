/* 
 * Pied de page 
 */
#footer {
    background-color: #157FCC;
    font: normal 11px arial,tahoma,sans-serif;
    color: #fff;
    padding: 5px 5px 5px 5px;
    height: 20px;
    position: absolute;

}

#mainFooter {
    background-color: #157FCC;
    font: normal 11px arial,tahoma,sans-serif;
    color: #fff;
    padding: 5px 5px 5px 5px;
    height: 20px;
}

#mainFooter .version{
    text-align: left;
    position: absolute;
    /*left: 5px;*/
}

#mainFooter .w3c{
    text-align: center;
    position: absolute;
    left: 40%;
    width: 165px;
    height: 20px;
}
#mainFooter .w3c img{
    width: 70px;
    height: 16px;
}
#mainFooter .xiti{
    text-align: center;
    position: absolute;
    right: 40%;
    width: 65px;
    height: 18px;
}
#mainFooter .copyright{
    text-align: right;
    position: absolute;
    float: right;
    right: 5px;
}
#mainFooter .copyright-txt{
    text-align: right;
    position: absolute;
    float: right;
    right: 90px;
}
